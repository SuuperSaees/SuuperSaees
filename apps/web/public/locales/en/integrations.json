{
  "title": "Integrations",
  "form": {
    "title": "Embed other apps",
    "description": "Embed your favorite apps in your project. Video gide.",
    "inputs": {
      "title": {
        "type": "text",
        "label": "Title",
        "placeholder": "Short title"
      },
      "icon": {
        "type": "text",
        "label": "Icon",
        "placeholder": "Icon URL"
      },
      "location": {
        "type": "select",
        "label": "Location",
        "options": [
          {
            "label": "Tab",
            "value": "tab"
          },
          {
            "label": "Sidebar",
            "value": "sidebar"
          }
        ]
      },
      "type": {
        "type": "select",
        "label": "Type",
        "options": [
          {
            "label": "Link",
            "value": "link"
          },
          {
            "label": "Iframe",
            "value": "iframe"
          }
        ]
      },
      "url": {
        "type": "text",
        "label": "Embed source",
        "placeholder": "Enter the URL or the iframe code"
      },
      "visibility": {
        "type": "select",
        "label": "Visibility",
        "options": [
          {
            "label": "For everyone",
            "value": "public",
            "icon": "Globe"
          },
          {
            "label": "Private",
            "value": "private",
            "icon": "Lock"
          }
        ]
      },
      "organizations": {
        "type": "select",
        "label": "Organizations",
        "placeholder": "Select organizations"
      }
    }
  }
}
